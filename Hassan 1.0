    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // --- JAVASCRIPT FOR MOBILE NAVIGATION TOGGLE (from Part 2) ---
            const menuToggle = document.getElementById('mobile-menu-toggle');
            const mobileNav = document.getElementById('mobile-nav-menu');
            const closeBtn = document.getElementById('close-mobile-nav');
            const mobileNavLinks = document.querySelectorAll('.mobile-nav ul li a');

            if (menuToggle && mobileNav && closeBtn) {
                menuToggle.addEventListener('click', () => {
                    mobileNav.classList.toggle('open');
                    menuToggle.classList.toggle('active');
                    document.body.style.overflow = mobileNav.classList.contains('open') ? 'hidden' : '';
                });
                closeBtn.addEventListener('click', () => {
                    mobileNav.classList.remove('open');
                    menuToggle.classList.remove('active');
                    document.body.style.overflow = '';
                });
                mobileNavLinks.forEach(link => {
                    link.addEventListener('click', () => {
                        mobileNav.classList.remove('open');
                        menuToggle.classList.remove('active');
                        document.body.style.overflow = '';
                    });
                });
                document.addEventListener('click', (event) => {
                    if (!mobileNav.contains(event.target) && !menuToggle.contains(event.target) && mobileNav.classList.contains('open')) {
                        mobileNav.classList.remove('open');
                        menuToggle.classList.remove('active');
                        document.body.style.overflow = '';
                    }
                });
            }

            // --- JAVASCRIPT FOR PRODUCT DETAIL PAGE (PDP) INTERACTIONS (NEW) ---
            const mainProductImage = document.getElementById('main-product-image');
            const thumbnails = document.querySelectorAll('.thumbnail-gallery .thumbnail');

            thumbnails.forEach(thumbnail => {
                thumbnail.addEventListener('click', () => {
                    // Remove 'active' class from all thumbnails
                    thumbnails.forEach(thumb => thumb.classList.remove('active'));
                    // Add 'active' class to the clicked thumbnail
                    thumbnail.classList.add('active');
                    // Change the main image source
                    mainProductImage.src = thumbnail.dataset.full;
                });
            });

            const sizeOptions = document.querySelectorAll('#size-options .option-button');
            sizeOptions.forEach(button => {
                button.addEventListener('click', () => {
                    if (!button.classList.contains('disabled')) {
                        sizeOptions.forEach(btn => btn.classList.remove('active'));
                        button.classList.add('active');
                        // In a real scenario, this would update selected size for add to cart
                        console.log('Selected Size:', button.dataset.value);
                    }
                });
            });

            const colorOptions = document.querySelectorAll('#color-options .color-option');
            colorOptions.forEach(button => {
                button.addEventListener('click', () => {
                    if (!button.classList.contains('disabled')) {
                        colorOptions.forEach(btn => btn.classList.remove('active'));
                        button.classList.add('active');
                        // In a real scenario, this would update selected color for add to cart
                        console.log('Selected Color:', button.dataset.color);
                    }
                });
            });

            const decreaseQtyBtn = document.getElementById('decrease-qty');
            const increaseQtyBtn = document.getElementById('increase-qty');
            const productQtyInput = document.getElementById('product-qty');

            if (decreaseQtyBtn && increaseQtyBtn && productQtyInput) {
                decreaseQtyBtn.addEventListener('click', () => {
                    let currentVal = parseInt(productQtyInput.value);
                    if (currentVal > productQtyInput.min) {
                        productQtyInput.value = currentVal - 1;
                    }
                });

                increaseQtyBtn.addEventListener('click', () => {
                    let currentVal = parseInt(productQtyInput.value);
                    if (currentVal < productQtyInput.max) {
                        productQtyInput.value = currentVal + 1;
                    }
                });

                // Prevent manual input of non-numbers or values outside min/max
                productQtyInput.addEventListener('change', () => {
                    let val = parseInt(productQtyInput.value);
                    if (isNaN(val) || val < productQtyInput.min) {
                        productQtyInput.value = productQtyInput.min;
                    } else if (val > productQtyInput.max) {
                        productQtyInput.value = productQtyInput.max;
                    }
                });
            }

            const tabButtons = document.querySelectorAll('.tab-button');
            const tabContents = document.querySelectorAll('.tab-content');

            tabButtons.forEach(button => {
                button.addEventListener('click', () => {
                    const targetTabId = button.dataset.tab;

                    // Deactivate all buttons and content
                    tabButtons.forEach(btn => btn.classList.remove('active'));
                    tabContents.forEach(content => content.classList.remove('active'));

                    // Activate clicked button and corresponding content
                    button.classList.add('active');
                    document.getElementById(targetTabId).classList.add('active');
                });
            });

            // Add to Cart functionality (conceptual)
            const addToCartBtn = document.querySelector('.add-to-cart-btn');
            if (addToCartBtn) {
                addToCartBtn.addEventListener('click', () => {
                    const selectedSize = document.querySelector('#size-options .option-button.active')?.dataset.value || 'N/A';
                    const selectedColor = document.querySelector('#color-options .color-option.active')?.dataset.color || 'N/A';
                    const quantity = document.getElementById('product-qty')?.value || 1;
                    const productName = document.querySelector('.product-details h1')?.textContent;

                    alert(`Added to cart: ${quantity} x ${productName} (Size: ${selectedSize}, Color: ${selectedColor})\n\n(This is a placeholder. In a real site, this would add to a cart state or send to a backend.)`);
                });
            }

            // Wishlist functionality (conceptual)
            const wishlistBtn = document.querySelector('.wishlist-btn');
            if (wishlistBtn) {
                wishlistBtn.addEventListener('click', () => {
                    wishlistBtn.classList.toggle('active'); // Example: visually indicate it's active
                    if (wishlistBtn.classList.contains('active')) {
                        wishlistBtn.innerHTML = '<i class="fas fa-heart"></i> Added to Wishlist'; // Solid heart icon
                        alert('Product added to your wishlist!');
                    } else {
                        wishlistBtn.innerHTML = '<i class="far fa-heart"></i> Add to Wishlist'; // Outline heart icon
                        alert('Product removed from your wishlist.');
                    }
                });
            }
        });
    </script>

