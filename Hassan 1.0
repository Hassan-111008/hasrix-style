        const sizeOptions = document.querySelectorAll('#size-options .option-button');
        sizeOptions.forEach(button => {
            button.addEventListener('click', () => {
                if (!button.classList.contains('disabled')) {
                    sizeOptions.forEach(btn => btn.classList.remove('active'));
                    button.classList.add('active');
                    console.log('Selected Size:', button.dataset.value);
                }
            });
        });

        const colorOptions = document.querySelectorAll('#color-options .color-option');
        colorOptions.forEach(button => {
            button.addEventListener('click', () => {
                if (!button.classList.contains('disabled')) {
                    colorOptions.forEach(btn => btn.classList.remove('active'));
                    button.classList.add('active');
                    console.log('Selected Color:', button.dataset.color);
                }
            });
        });

        const decreaseQtyBtn = document.getElementById('decrease-qty');
        const increaseQtyBtn = document.getElementById('increase-qty');
        const productQtyInput = document.getElementById('product-qty');

        if (decreaseQtyBtn && increaseQtyBtn && productQtyInput) {
            decreaseQtyBtn.addEventListener('click', () => {
                let currentVal = parseInt(productQtyInput.value);
                if (currentVal > parseInt(productQtyInput.min)) {
                    productQtyInput.value = currentVal - 1;
                }
            });

            increaseQtyBtn.addEventListener('click', () => {
                let currentVal = parseInt(productQtyInput.value);
                if (currentVal < parseInt(productQtyInput.max)) {
                    productQtyInput.value = currentVal + 1;
                }
            });

            productQtyInput.addEventListener('change', () => {
                let val = parseInt(productQtyInput.value);
                if (isNaN(val) || val < parseInt(productQtyInput.min)) {
                    productQtyInput.value = productQtyInput.min;
                } else if (val > parseInt(productQtyInput.max)) {
                    productQtyInput.value = productQtyInput.max;
                }
            });
        }

        const tabButtons = document.querySelectorAll('.tab-button');
        const tabContents = document.querySelectorAll('.tab-content');

        tabButtons.forEach(button => {
            button.addEventListener('click', () => {
                const targetTabId = button.dataset.tab;

                tabButtons.forEach(btn => btn.classList.remove('active'));
                tabContents.forEach(content => content.classList.remove('active'));

                button.classList.add('active');
                document.getElementById(targetTabId).classList.add('active');
            });
        });

        // --- ADD TO CART FUNCTIONALITY (NEW - from Part 7, part of PDP script) ---
        const addToCartBtn = document.querySelector('.add-to-cart-btn');
        if (addToCartBtn) {
            addToCartBtn.addEventListener('click', () => {
                const productName = document.querySelector('.product-details h1')?.textContent;
                const productPrice = parseFloat(document.querySelector('.product-details .price')?.textContent.replace('$', ''));
                const productImage = document.getElementById('main-product-image')?.src;
                const selectedSizeElement = document.querySelector('#size-options .option-button.active');
                const selectedSize = selectedSizeElement ? selectedSizeElement.dataset.value : 'N/A';
                const selectedColorElement = document.querySelector('#color-options .color-option.active');
                const selectedColor = selectedColorElement ? selectedColorElement.dataset.color : 'N/A';
                const quantity = parseInt(productQtyInput.value);

                if (!selectedSize || selectedSize === 'N/A') {
                    alert('Please select a size before adding to cart.');
                    return;
                }
                if (!selectedColor || selectedColor === 'N/A') {
                    alert('Please select a color before adding to cart.');
                    return;
                }

                const item = {
                    id: `${productName.replace(/\s/g, '-')}-${selectedSize}-${selectedColor}`, // Unique ID for prototyping
                    name: productName,
                    price: productPrice,
                    image: productImage,
                    size: selectedSize,
                    color: selectedColor,
                    quantity: quantity
                };

                addItemToCart(item);
                alert(`${quantity} x ${productName} (Size: ${selectedSize}, Color: ${selectedColor}) added to cart!`);
            });
        }

        // Wishlist functionality (from Part 6)
        const wishlistBtn = document.querySelector('.wishlist-btn');
        if (wishlistBtn) {
            wishlistBtn.addEventListener('click', () => {
                wishlistBtn.classList.toggle('active');
                if (wishlistBtn.classList.contains('active')) {
                    wishlistBtn.innerHTML = '<i class="fas fa-heart"></i> Added to Wishlist';
                    alert('Product added to your wishlist!');
                } else {
                    wishlistBtn.innerHTML = '<i class="far fa-heart"></i> Add to Wishlist';
                    alert('Product removed from your wishlist.');
                }
            });
        }
    } // End of PDP specific JS


    // --- GLOBAL CART MANAGEMENT FUNCTIONS (NEW - Applies to all pages) ---

    // Elements that need to reflect cart count across different pages
    const headerCartCountSpans = document.querySelectorAll('.cart-count');
    const cartTableBody = document.getElementById('cart-table-body'); // Only exists on cart.html
    const cartSubtotalSpan = document.getElementById('cart-subtotal'); // Only exists on cart.html
    const cartGrandTotalSpan = document.getElementById('cart-grand-total'); // Only exists on cart.html
    const cartEmptyMessage = document.getElementById('cart-empty-message'); // Only exists on cart.html
    const cartSummaryContainer = document.getElementById('cart-summary-container'); // Only exists on cart.html


    // Function to load cart from localStorage
    const loadCart = () => {
        try {
            const cart = JSON.parse(localStorage.getItem('hasrixCart')) || [];
            return cart;
        } catch (e) {
            console.error("Error loading cart from localStorage:", e);
            return [];
        }
    };

    // Function to save cart to localStorage and update UI
    const saveCart = (cart) => {
        localStorage.setItem('hasrixCart', JSON.stringify(cart));
        updateCartUI(cart); // Update UI after saving
    };

    // Function to add item to cart
    const addItemToCart = (newItem) => {
        let cart = loadCart();
        const existingItemIndex = cart.findIndex(item =>
            item.id === newItem.id // Check for identical product, size, and color
        );

        if (existingItemIndex > -1) {
            // Item exists, just update quantity
            cart[existingItemIndex].quantity += newItem.quantity;
            if (cart[existingItemIndex].quantity > 10) { // Max quantity limit
                cart[existingItemIndex].quantity = 10;
                alert('Maximum quantity per item reached (10).');
            }
        } else {
            // New item, add to cart
            cart.push(newItem);
        }
        saveCart(cart); // Save and update UI
    };

    // Function to remove item from cart
    const removeItemFromCart = (index) => {
        let cart = loadCart();
        if (index > -1 && index < cart.length) {
            cart.splice(index, 1);
            saveCart(cart);
        }
    };

    // Function to update item quantity in cart
    const updateItemQuantity = (index, newQuantity) => {
        let cart = loadCart();
        if (index > -1 && index < cart.length) {
            let qty = parseInt(newQuantity);
            if (isNaN(qty) || qty < 1) qty = 1;
            if (qty > 10) qty = 10; // Max quantity limit
            cart[index].quantity = qty;
            saveCart(cart);
        }
    };

    // Function to update cart UI (table, totals, count)
    // This function will intelligently check if the elements exist on the current page
    const updateCartUI = (cart) => {
        // Update header cart count on ALL pages
        headerCartCountSpans.forEach(span => {
            span.textContent = cart.length;
        });

        // Only update cart table and totals if on the cart.html page
        if (cartTableBody && cartSubtotalSpan && cartGrandTotalSpan) {
            cartTableBody.innerHTML = ''; // Clear existing items

            if (cart.length === 0) {
                cartEmptyMessage.style.display = 'block';
                cartSummaryContainer.style.display = 'none';
            } else {
                cartEmptyMessage.style.display = 'none';
                cartSummaryContainer.style.display = 'block';
                let totalSubtotal = 0;

                cart.forEach((item, index) => {
                    const row = document.createElement('tr');
                    const itemSubtotal = item.price * item.quantity;
                    totalSubtotal += itemSubtotal;

                    row.innerHTML = `
                        <td data-label="Product">
                            <div class="cart-item-details">
                                <img src="${item.image}" alt="${item.name}">
                                <div class="cart-item-info">
                                    <h4>${item.name}</h4>
                                    <p>Size: ${item.size}</p>
                                    <p>Color: ${item.color}</p>
                                </div>
                            </div>
                        </td>
                        <td data-label="Price" class="cart-item-price">$${item.price.toFixed(2)}</td>
                        <td data-label="Quantity">
                            <div class="cart-item-quantity">
                                <button class="decrease-qty" data-index="${index}">-</button>
                                <input type="number" value="${item.quantity}" min="1" max="10" data-index="${index}">
                                <button class="increase-qty" data-index="${index}">+</button>
                            </div>
                        </td>
                        <td data-label="Subtotal" class="cart-item-subtotal">$${itemSubtotal.toFixed(2)}</td>
                        <td data-label="Remove" class="cart-item-remove">
                            <button class="remove-item-btn" data-index="${index}"><i class="fas fa-times"></i></button>
                        </td>
                    `;
                    cartTableBody.appendChild(row);
                });

                cartSubtotalSpan.textContent = `$${totalSubtotal.toFixed(2)}`;
                // For a prototype, total = subtotal
                cartGrandTotalSpan.textContent = `$${totalSubtotal.toFixed(2)}`;

                // Add event listeners for new cart table buttons after rendering
                attachCartEventListeners();
            }
        }
    };

    // Function to attach event listeners to cart items (for quantity and remove) on the cart page
    const attachCartEventListeners = () => {
        // Only attach if on cart.html and elements exist
        if (cartTableBody) {
            document.querySelectorAll('.cart-item-quantity .decrease-qty').forEach(button => {
                button.onclick = (e) => {
                    const index = e.target.dataset.index;
                    let cart = loadCart();
                    if (cart[index] && cart[index].quantity > 1) {
                        updateItemQuantity(index, cart[index].quantity - 1);
                    }
                };
            });

            document.querySelectorAll('.cart-item-quantity .increase-qty').forEach(button => {
                button.onclick = (e) => {
                    const index = e.target.dataset.index;
                    let cart = loadCart();
                    if (cart[index] && cart[index].quantity < 10) {
                        updateItemQuantity(index, cart[index].quantity + 1);
                    }
                };
            });

            document.querySelectorAll('.cart-item-quantity input[type="number"]').forEach(input => {
                input.onchange = (e) => {
                    const index = e.target.dataset.index;
                    updateItemQuantity(index, e.target.value);
                };
            });

            document.querySelectorAll('.remove-item-btn').forEach(button => {
                button.onclick = (e) => {
                    const index = e.target.closest('button').dataset.index;
                    removeItemFromCart(index);
                };
            });
        }
    };

    // Initial update of cart UI on page load (for all pages to show count)
    updateCartUI(loadCart());

}); // End of DOMContentLoaded
