    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // --- JAVASCRIPT FOR MOBILE NAVIGATION TOGGLE (from Part 2) ---
            const menuToggle = document.getElementById('mobile-menu-toggle');
            const mobileNav = document.getElementById('mobile-nav-menu');
            const closeBtn = document.getElementById('close-mobile-nav');
            const mobileNavLinks = document.querySelectorAll('.mobile-nav ul li a');

            if (menuToggle && mobileNav && closeBtn) {
                menuToggle.addEventListener('click', () => {
                    mobileNav.classList.toggle('open');
                    menuToggle.classList.toggle('active');
                    document.body.style.overflow = mobileNav.classList.contains('open') ? 'hidden' : '';
                });
                closeBtn.addEventListener('click', () => {
                    mobileNav.classList.remove('open');
                    menuToggle.classList.remove('active');
                    document.body.style.overflow = '';
                });
                mobileNavLinks.forEach(link => {
                    link.addEventListener('click', () => {
                        mobileNav.classList.remove('open');
                        menuToggle.classList.remove('active');
                        document.body.style.overflow = '';
                    });
                });
                document.addEventListener('click', (event) => {
                    if (!mobileNav.contains(event.target) && !menuToggle.contains(event.target) && mobileNav.classList.contains('open')) {
                        mobileNav.classList.remove('open');
                        menuToggle.classList.remove('active');
                        document.body.style.overflow = '';
                    }
                });
            }

            // --- JAVASCRIPT FOR CART FUNCTIONALITY (NEW) ---
            const cartTableBody = document.getElementById('cart-table-body');
            const cartSubtotalSpan = document.getElementById('cart-subtotal');
            const cartGrandTotalSpan = document.getElementById('cart-grand-total');
            const cartEmptyMessage = document.getElementById('cart-empty-message');
            const cartSummaryContainer = document.getElementById('cart-summary-container');
            const headerCartCountSpans = document.querySelectorAll('.cart-count'); // Select all elements with this class

            // Function to load cart from localStorage
            const loadCart = () => {
                const cart = JSON.parse(localStorage.getItem('hasrixCart')) || [];
                return cart;
            };

            // Function to save cart to localStorage
            const saveCart = (cart) => {
                localStorage.setItem('hasrixCart', JSON.stringify(cart));
                updateCartUI(cart);
            };

            // Function to update cart UI (table, totals, count)
            const updateCartUI = (cart) => {
                cartTableBody.innerHTML = ''; // Clear existing items

                if (cart.length === 0) {
                    cartEmptyMessage.style.display = 'block';
                    cartSummaryContainer.style.display = 'none';
                } else {
                    cartEmptyMessage.style.display = 'none';
                    cartSummaryContainer.style.display = 'block';
                    let totalSubtotal = 0;

                    cart.forEach((item, index) => {
                        const row = document.createElement('tr');
                        const itemSubtotal = item.price * item.quantity;
                        totalSubtotal += itemSubtotal;

                        row.innerHTML = `
                            <td data-label="Product">
                                <div class="cart-item-details">
                                    <img src="${item.image}" alt="${item.name}">
                                    <div class="cart-item-info">
                                        <h4>${item.name}</h4>
                                        <p>Size: ${item.size}</p>
                                        <p>Color: ${item.color}</p>
                                    </div>
                                </div>
                            </td>
                            <td data-label="Price" class="cart-item-price">$${item.price.toFixed(2)}</td>
                            <td data-label="Quantity">
                                <div class="cart-item-quantity">
                                    <button class="decrease-qty" data-index="${index}">-</button>
                                    <input type="number" value="${item.quantity}" min="1" max="10" data-index="${index}">
                                    <button class="increase-qty" data-index="${index}">+</button>
                                </div>
                            </td>
                            <td data-label="Subtotal" class="cart-item-subtotal">$${itemSubtotal.toFixed(2)}</td>
                            <td data-label="Remove" class="cart-item-remove">
                                <button class="remove-item-btn" data-index="${index}"><i class="fas fa-times"></i></button>
                            </td>
                        `;
                        cartTableBody.appendChild(row);
                    });

                    // Update totals
                    cartSubtotalSpan.textContent = `$${totalSubtotal.toFixed(2)}`;
                    // For a prototype, let's keep shipping simple or indicate it's calculated later
                    // cartShippingSpan.textContent = `$5.00`; // Example fixed shipping
                    cartGrandTotalSpan.textContent = `$${totalSubtotal.toFixed(2)}`; // For prototype, total = subtotal
                }

                // Update cart count in header and mobile nav
                headerCartCountSpans.forEach(span => {
                    span.textContent = cart.length;
                });

                // Add event listeners for new buttons after rendering
                attachCartEventListeners();
            };

            // Function to attach event listeners to cart items (for quantity and remove)
            const attachCartEventListeners = () => {
                document.querySelectorAll('.cart-item-quantity .decrease-qty').forEach(button => {
                    button.onclick = (e) => {
                        const index = e.target.dataset.index;
                        let cart = loadCart();
                        if (cart[index].quantity > 1) {
                            cart[index].quantity--;
                            saveCart(cart);
                        }
                    };
                });

                document.querySelectorAll('.cart-item-quantity .increase-qty').forEach(button => {
                    button.onclick = (e) => {
                        const index = e.target.dataset.index;
                        let cart = loadCart();
                        if (cart[index].quantity < 10) { // Max quantity 10 for prototype
                            cart[index].quantity++;
                            saveCart(cart);
                        }
                    };
                });

                document.querySelectorAll('.cart-item-quantity input[type="number"]').forEach(input => {
                    input.onchange = (e) => {
                        const index = e.target.dataset.index;
                        let cart = loadCart();
                        let newQty = parseInt(e.target.value);
                        if (isNaN(newQty) || newQty < 1) newQty = 1;
                        if (newQty > 10) newQty = 10; // Max quantity 10 for prototype
                        cart[index].quantity = newQty;
                        saveCart(cart);
                    };
                });

                document.querySelectorAll('.remove-item-btn').forEach(button => {
                    button.onclick = (e) => {
                        const index = e.target.closest('button').dataset.index;
                        let cart = loadCart();
                        cart.splice(index, 1); // Remove item from array
                        saveCart(cart);
                    };
                });
            };

            // Initial load of cart when page loads
            updateCartUI(loadCart());

        });
    </script>
</body>
