            background-position: right 10px center;
            background-size: 12px;
            padding-right: 30px; /* Space for custom arrow */
        }
        .sort-options select option {
            background-color: #1A1A1A; /* Option background */
            color: #FFFFFF; /* Option text */
        }


        /* Product Grid (from Part 3, re-used) */
        .product-list-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr)); /* Slightly smaller min-width for PLP */
            gap: 30px;
            padding-bottom: 60px;
        }

        .product-card { /* Re-using styles from Part 3 */
            background-color: #1A1A1A;
            border: 1px solid #222;
            padding: 20px;
            text-align: center;
            transition: transform 0.3s ease, box-shadow 0.3s ease;
            overflow: hidden;
