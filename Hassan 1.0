        .product-card .quick-view-btn {
            display: inline-block;
            background-color: transparent;
            color: #FFD700;
            border: 1px solid #FFD700;
            padding: 10px 25px;
            font-size: 0.9em;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.08em;
