        }

        /* --- UTILITY CLASSES --- */
        .container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 20px;
        }

        .text-gold {
            color: #FFD700; /* Gold accent */
        }

        /* --- HERO SECTION STYLES (will be in a separate _hero.scss or similar) --- */
        .hero-section {
            width: 100vw;
            height: 100vh; /* Full viewport height */
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            text-align: center;
            position: relative;
            /* Placeholder for background video/image. Replace with your actual assets. */
            background-image: url('https://via.placeholder.com/1920x1080/0A0A0A/E0E0E0?text=HASRIX+Luxury+Sportswear+Hero');
            background-size: cover;
            background-position: center;
            background-repeat: no-repeat;
            overflow: hidden;
            color: #FFFFFF; /* Ensure text is white */
        }

        .hero-overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.65); /* Darker overlay for better text contrast */
            z-index: 1;
        }

        .hero-content {
            position: relative;
            z-index: 2; /* Content above overlay */
