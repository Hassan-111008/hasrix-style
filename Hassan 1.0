        }

        .product-card .quick-view-btn:hover {
            background-color: #FFD700;
            color: #0A0A0A;
            border-color: #0A0A0A;
        }

        /* Pagination (conceptual) */
        .pagination {
            display: flex;
            justify-content: center;
            margin-top: 40px;
            padding-bottom: 80px;
        }

        .pagination a {
            display: inline-block;
            margin: 0 8px;
            padding: 10px 15px;
            border: 1px solid #333;
            color: #E0E0E0;
            border-radius: 3px;
            transition: background-color 0.3s ease, color 0.3s ease, border-color 0.3s ease;
        }

        .pagination a:hover,
        .pagination a.current {
            background-color: #FFD700;
            color: #0A0A0A;
            border-color: #FFD700;
        }


        /* --- RESPONSIVE ADJUSTMENTS --- */
        @media (max-width: 1024px) {
            .shop-layout {
                grid-template-columns: 240px 1fr; /* Smaller sidebar */
                gap: 30px;
            }
